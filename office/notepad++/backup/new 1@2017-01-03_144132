 SELECT                            array_to_json(array_agg(t))   FROM                              (      SELECT                                                                                                                                                                                            round(sum(extract(epoch from(to_timestamp(cdr_historico.fecha_final, 'YYYY-MM-DD HH24:MI:SS')-to_timestamp(cdr_historico.fecha_inicio, 'YYYY-MM-DD HH24:MI:SS')))) / 60) AS time_minutes       , COUNT(*) AS meetings                                                                                                                                                                            , CAST( sesion_usuario_historico.country_code AS character varying(100) ) AS group                                                                                                                                                   , COUNTRY AS filter                                                                                                                                                 FROM                                                                                                                                                                                              cdr_historico                                                                                                                                                                                 INNER JOIN                                                                                                                                                                                        sesion_usuario_historico                                                                                                                                                                      ON                                                                                                                                                                                                cdr_historico.id = sesion_usuario_historico.id_cdr                                                                                                                                            AND                                                                                                                                                                                               cdr_historico.nombre_entidad =  ( SELECT nombre_conexion FROM entidad WHERE id = 9 )   AND                                                                                                                                                                                             cdr_historico.fecha_inicio >= '2016-01-01 00:00:00'                                                                                                                                                             AND                                                                                                                                                                                             cdr_historico.fecha_inicio <= '2017-01-01 23:59:59'                                                                                                                                                             GROUP BY                                                                                                                                                                                          sesion_usuario_historico.country_code                                                                                                                                                      UNION      SELECT                                                                                                                                                                                            round(sum(extract(epoch from(to_timestamp(cdr_historico.fecha_final, 'YYYY-MM-DD HH24:MI:SS')-to_timestamp(cdr_historico.fecha_inicio, 'YYYY-MM-DD HH24:MI:SS')))) / 60) AS time_minutes       , COUNT(*) AS meetings                                                                                                                                                                            , CAST( sesion_usuario_historico.id_plataforma AS character varying(100) ) AS group                                                                                                                                                   , DEVICE AS filter                                                                                                                                                 FROM                                                                                                                                                                                              cdr_historico                                                                                                                                                                                 INNER JOIN                                                                                                                                                                                        sesion_usuario_historico                                                                                                                                                                      ON                                                                                                                                                                                                cdr_historico.id = sesion_usuario_historico.id_cdr                                                                                                                                            AND                                                                                                                                                                                               cdr_historico.nombre_entidad =  ( SELECT nombre_conexion FROM entidad WHERE id = 9 )   AND                                                                                                                                                                                             cdr_historico.fecha_inicio >= '2016-01-01 00:00:00'                                                                                                                                                             AND                                                                                                                                                                                             cdr_historico.fecha_inicio <= '2017-01-01 23:59:59'                                                                                                                                                             GROUP BY                                                                                                                                                                                          sesion_usuario_historico.id_plataforma                                                                                                                                                      ) AS t                  