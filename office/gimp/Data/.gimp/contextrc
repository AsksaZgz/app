# GIMP user context

(tool "gimp-by-color-select-tool")
(paint-info "gimp-paintbrush")
(foreground (color-rgb 1.000000 1.000000 0.000000))
(brush "1. Pixel")
(dynamics "Dynamics Off")
(mybrush "100% Opaque")
(pattern "Pine")
(gradient "FG to BG (RGB)")
(palette "Color History")
(font "Sans-serif")
(tool-preset "Core Pointer")

# end of user context
